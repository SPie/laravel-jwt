{
    "name": "spie/laravel-jwt",
    "description": "JWT package for Laravel and Lumen.",
    "keywords": ["laravel", "lumen", "jwt", "authentication"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">7.1.3",
        "lcobucci/jwt": "^3.2",
        "illuminate/auth": "^5.7",
        "illuminate/console": "^5.7",
        "illuminate/contracts": "^5.7",
        "illuminate/support": "^5.7",
        "illuminate/cache": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "SPie\\LaravelJWT\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
