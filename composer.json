{
    "name": "spie/laravel-jwt",
    "description": "JWT package for Laravel and Lumen.",
    "keywords": ["laravel", "lumen", "jwt", "authentication"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">7.1.3",
        "lcobucci/jwt": "^3.2",
        "illuminate/auth": ">=5.4",
        "illuminate/config": ">=5.4",
        "illuminate/console": ">=5.4",
        "illuminate/contracts": ">=5.4",
        "illuminate/support": ">=5.4",
        "illuminate/cache": ">=5.4",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "SPie\\LaravelJWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SPie\\LaravelJWT\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
