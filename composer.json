{
    "name": "spie/laravel-jwt",
    "description": "JWT package for Laravel and Lumen.",
    "keywords": ["laravel", "lumen", "jwt", "authentication"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "illuminate/auth": ">=7.0",
        "illuminate/config": ">=7.0",
        "illuminate/console": ">=7.0",
        "illuminate/contracts": ">=7.0",
        "illuminate/support": ">=7.0",
        "illuminate/cache": ">=7.0",
        "lcobucci/jwt": "^4.0",
        "nesbot/carbon": "^2.0",
        "vlucas/phpdotenv": "^4.0 || ^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.12.90",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/extension-installer": "^1.1",
        "fakerphp/faker": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "SPie\\LaravelJWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SPie\\LaravelJWT\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpstan/extension-installer": false
        }
    }
}
